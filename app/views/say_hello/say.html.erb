<script>
  function initMap() {
    var agawam = {lat: 42.070206, lng:-72.622739 };
    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 6,
      center: agawam
    });
    var marker = new google.maps.Marker({
      position: agawam,
      map: map
    });
  }
</script>

